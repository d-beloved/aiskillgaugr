{
  "version": 2,
  "buildCommand": "pnpm build",
  "devCommand": "pnpm dev",
  "installCommand": "pnpm install",
  "framework": "vite",
  "regions": ["sfo1"],
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/server/index.ts"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/"
    }
  ]
}
