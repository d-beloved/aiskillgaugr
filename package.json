{
  "name": "aiskillguagr",
  "version": "0.0.0",
  "description": "A AI powered dev skill guager",
  "scripts": {
    "dev": "concurrently \"vike dev\" \"tsx watch server/index.ts\"",
    "build": "pnpm run test && pnpm run build:client && pnpm run build:server",
    "build:server": "tsc -p server/tsconfig.json --skipLibCheck && cp server/app.ts dist/server/",
    "build:client": "vike build",
    "start": "concurrently \"node --experimental-json-modules dist/server/entry.mjs\" \"vike preview\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint ."
  },
  "dependencies": {
    "@huggingface/inference": "^3.3.6",
    "@vite-plugin-vercel/vike": "^9.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.0.0",
    "vike-react": "^0.5.13",
    "vite-plugin-vercel": "^9.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@tailwindcss/vite": "^4.0.6",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.0.9",
    "@types/react-dom": "^19.0.3",
    "@vercel/node": "^2.15.10",
    "@vitest/coverage-v8": "^3.0.7",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "concurrently": "^9.1.2",
    "daisyui": "^5.0.0-beta.8",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^15.15.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.1",
    "tailwindcss": "^4.0.6",
    "tsx": "^4.19.3",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0",
    "vike": "^0.4.223",
    "vite": "^6.1.0",
    "vitest": "^3.0.7"
  },
  "type": "module",
  "license": "MIT",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
